from SinglyLinkedLists import LinkedList

def move_tail_to_head(self):
        if self.head and self.head.next:
            last = self.head
            prev = None
            
            while last.next:
                prev = last
                last = last.next
            
            last.next = self.head
            prev.next = None
            self.head = last

llist = LinkedList()
llist.append("A")
llist.append("B")
llist.append("C")
llist.append("D")

llist.print_list()
move_tail_to_head(llist)
print("\n")
llist.print_list()